#import "ParticleAndDepth.h"

@implementation ParticleAndDepth

@synthesize particle = _particle;
@synthesize depth = _depth;

-(id)initWith:(Particle *)t depth:(int)depth
{
    self = [super init];
    
    if (self) {
        self.particle = t;
        self.depth = depth;
    }
    
    return self;
}

@end
